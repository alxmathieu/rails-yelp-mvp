<h1>All Reviews</h1>

<ul></ul>
  <% @reviews.each do |review| %>
    <li> <%= review.content %> || Rating: <%= review.rating %>/5 </li>
  <% end %>

<%= link_to "Back to the restaurant", restaurant_path(@restaurant), class: "btn btn-primary"  %>
<%= link_to "Add a review", new_restaurant_review_path(@restaurant), class: "btn btn-primary"  %>
